{
  "name": "@ant-design/charts",
  "version": "0.2.9",
  "description": "React translate g2plot",
  "files": [
    "/lib",
    "/es"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "scripts": {
    "start": "tnpm run dev",
    "dev": "bigfish dev",
    "build": "tnpm run build:lib && tnpm run build:tsc",
    "build:lib": "bigfish lib build",
    "build:umd": "BUILD_ENV=umd bigfish lib build",
    "build:tsc": "tsc --declarationDir ./lib && tsc --declarationDir ./es",
    "ci": "bigfish lint && bigfish cov",
    "cov": "bigfish cov",
    "help": "bigfish help",
    "lint": "bigfish lint --style --eslint.ext='.ts,.tsx,.jsx,.js'",
    "precommit": "bigfish lint --staged --prettier --style --fix --eslint.ext='.ts,.tsx,.jsx,.js'",
    "pub": "tnpm run build && node ./scripts/publish.js",
    "pub:doc": "bigfish lib doc publish",
    "test": "bigfish test",
    "version": "bigfish -v"
  },
  "dependencies": {
    "@alipay/bigfish": "^2.28.4",
    "@antv/g2plot": "^1.0.2"
  },
  "devDependencies": {
    "@alipay/bigfish": "^2.24.2-alpha.5",
    "@types/node": "^12.12.17",
    "@types/webpack-env": "^1.14.1",
    "shelljs": "^0.8.3",
    "typescript": "^3.7.4",
    "umi-plugin-father-doc": "^1.0.0-alpha.20"
  },
  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "engines": {
    "install-alinode": "4"
  },
  "publishConfig": {
    "registry": "http://registry.npm.alibaba-inc.com"
  },
  "ci": {
    "version": "alinode=4"
  }
}
