{
  "name": "@ant-design/charts",
  "version": "0.9.1",
  "description": "React translate g2plot",
  "files": [
    "/lib",
    "/es"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ant-design/ant-design-charts.git"
  },
  "bugs": {
    "url": "https://github.com/ant-design/ant-design-charts/issues"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "scripts": {
    "start": "npm run dev",
    "dev": "dumi dev",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "build": "father-build",
    "pub": "npm run build && np",
    "addChart": "node ./scripts/addChart.js",
    "entry": "node ./scripts/entry.js"
  },
  "dependencies": {
    "@antv/g2plot": "^1.0.3"
  },
  "devDependencies": {
    "@types/node": "^12.12.17",
    "@types/webpack-env": "^1.14.1",
    "@umijs/fabric": "^2.0.7",
    "@umijs/preset-react": "^1.4.8",
    "@umijs/test": "^3.0.16",
    "dumi": "^1.0.13",
    "ejs": "^3.0.2",
    "father-build": "^1.17.2",
    "np": "*",
    "prettier": "^2.0.2",
    "shelljs": "^0.8.3",
    "typescript": "^3.7.4"
  },
  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "engines": {
    "install-alinode": "4"
  },
  "np": {
    "yarn": false,
    "contents": "dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "ci": {
    "version": "alinode=4"
  }
}
